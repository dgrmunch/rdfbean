@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix foo: <http://www.foo.com/foo.owl#> .

{
	<http://www.foo.com/Job/1> rdf:type foo:Job .
	<http://www.foo.com/Job/1> foo:jobId "1"^^xsd:string .
	<http://www.foo.com/Job/1> foo:jobItem <http://www.foo.com/JobItem/1> .
	
	<http://www.foo.com/JobItem/1> rdf:type foo:JobItem .
	<http://www.foo.com/JobItem/1> foo:jobItemId "1"^^xsd:string .
	<http://www.foo.com/JobItem/1> foo:repositoryContext <http://www.foo.com/RepositoryContext/test> . 
}